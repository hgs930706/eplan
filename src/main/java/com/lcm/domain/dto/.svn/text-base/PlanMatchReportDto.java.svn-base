package com.lcm.domain.dto;

import java.time.LocalDate;
import java.util.List;

public class PlanMatchReportDto {

    private String site;

    private String fab;

    private LocalDate shiftDate;

    private String shift;

    private int peopleLineTotal;//人员排线总数

    private int systmeLineTotal;//系统排线总数

    private int okTotal;//排产OK数

    private int total;//线体总数

    private String ppl;//匹配率

    private Double pplChart;//图表匹配率

    private List<PlanMatchPplDto> planMatchPplDtoList;

    public List<PlanMatchPplDto> getPlanMatchPplDtoList() {
        return planMatchPplDtoList;
    }

    public void setPlanMatchPplDtoList(List<PlanMatchPplDto> planMatchPplDtoList) {
        this.planMatchPplDtoList = planMatchPplDtoList;
    }

    public Double getPplChart() {
        return pplChart;
    }

    public void setPplChart(Double pplChart) {
        this.pplChart = pplChart;
    }

    public int getTotal() {
        return total;
    }

    public void setTotal(int total) {
        this.total = total;
    }

    public String getPpl() {
        return ppl;
    }

    public void setPpl(String ppl) {
        this.ppl = ppl;
    }

    public String getSite() {
        return site;
    }

    public void setSite(String site) {
        this.site = site;
    }

    public String getFab() {
        return fab;
    }

    public void setFab(String fab) {
        this.fab = fab;
    }

    public LocalDate getShiftDate() {
        return shiftDate;
    }

    public void setShiftDate(LocalDate shiftDate) {
        this.shiftDate = shiftDate;
    }

    public String getShift() {
        return shift;
    }

    public void setShift(String shift) {
        this.shift = shift;
    }

    public int getPeopleLineTotal() {
        return peopleLineTotal;
    }

    public void setPeopleLineTotal(int peopleLineTotal) {
        this.peopleLineTotal = peopleLineTotal;
    }

    public int getSystmeLineTotal() {
        return systmeLineTotal;
    }

    public void setSystmeLineTotal(int systmeLineTotal) {
        this.systmeLineTotal = systmeLineTotal;
    }

    public int getOkTotal() {
        return okTotal;
    }

    public void setOkTotal(int okTotal) {
        this.okTotal = okTotal;
    }


}
