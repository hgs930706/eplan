<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">

        <changeSet id="1" author="rpy">
            <createTable tableName="R_JOB_PLAN">
                <column name="SITE" type="VARCHAR2(20)"></column>
                <column name="FAB" type="VARCHAR2(20)"></column>
                <column name="AREA" type="VARCHAR2(20)"></column>
                <column name="LINE" type="VARCHAR2(20)"></column>
                <column name="JOB_ID" type="VARCHAR2(50)">
                    <constraints primaryKey="true"></constraints>
                </column>
                <column name="SHIFT_DATE" type="DATE"></column>
                <column name="MODEL_NO" type="VARCHAR2(40)"></column>
                <column name="PART_NO" type="VARCHAR2(40)"></column>
                <column name="CELL_PART_NO" type="VARCHAR2(40)"></column>
                <column name="GRADE" type="VARCHAR2(1)"></column>
                <column name="PLAN_QTY" type="NUMBER"></column>
                <column name="JOB_TYPE" type="VARCHAR2(20)"></column>
                <column name="LM_USER" type="VARCHAR2(20)"></column>
                <column name="LM_TIME" type="DATE"></column>
            </createTable>
        </changeSet>
</databaseChangeLog>