                </column>
                <column name="SHIFT" type="VARCHAR2(1)">
                    <constraints primaryKey="true"></constraints>
                </column>
                <column name="JOB_TYPE" type="VARCHAR2(20)">
                    <constraints primaryKey="true"></constraints>
                </column>
                <column name="PART_LEVEL" type="VARCHAR2(40)">
                    <constraints primaryKey="true"></constraints>
                </column>
                <column name="MODEL_NO" type="VARCHAR2(40)">
                    <constraints primaryKey="true"></constraints>
                </column>
                <column name="PLAN_QTY" type="NUMBER"></column>
                <column name="LM_USER" type="VARCHAR2(20)"></column>
                <column name="LM_TIME" type="DATE"></column>
                <column name="RUN_SEQ" type="NUMBER"></column>
                <column name="WO_ID" type="VARCHAR2(20)">
                    <constraints primaryKey="true"></constraints>
                </column>
                <column name="PART_NO" type="VARCHAR2(40)"></column>
                <column name="CHANGE_LEVEL" type="VARCHAR2(20)"></column>
                <column name="DURATION" type="NUMBER"></column>
                <column name="CHANGE_KEY" type="VARCHAR2(30)">
                    <constraints primaryKey="true"></constraints>
                </column>
            </createTable>
        </changeSet>
</databaseChangeLog>