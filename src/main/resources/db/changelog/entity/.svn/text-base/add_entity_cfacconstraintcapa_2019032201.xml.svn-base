                </column>
                <column name="ITEM_VALUE" type="NUMBER"></column>
                <column name="LM_USER" type="VARCHAR2(20)"></column>
                <column name="LM_TIME" type="DATE"></column>
            </createTable>
        </changeSet>
</databaseChangeLog>